<?xml version="1.0" encoding="utf-8"?>
<com.example.recyclerviewexample.android.customveiw.TouchViewDemo xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_java_base"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="btn_java_base"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_custome_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="custome_view"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/custom_dialog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="custom_dialog"
        android:textAllCaps="false" />

</com.example.recyclerviewexample.android.customveiw.TouchViewDemo>