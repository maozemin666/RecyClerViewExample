        </activity>
        <activity android:name=".android.activity.LabelFloLayoutActivity" />
        <activity android:name=".android.activity.CustomeViewActivity" />
        <activity android:name=".android.activity.JavaBaseActivity" />
        <activity android:name=".android.activity.MainActivity">

