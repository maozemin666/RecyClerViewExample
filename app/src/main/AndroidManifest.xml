        <activity android:name=".android.activity.XUIActivity" />
        <activity android:name=".android.activity.KotlinActivity" />
        <activity android:name=".android.activity.JsBridgeWebViewActivity" />
        <activity android:name=".android.activity.WebViewActivity" />
        <activity android:name=".android.activity.PayDemoActivity" />
        <activity android:name=".android.activity.RetrofitActivity" />
        <activity android:name=".android.activity.MvpActivity" />
        <activity android:name=".android.activity.EventBusActivity" />
        <activity android:name=".android.activity.GlideActivity" />
        <activity android:name=".android.activity.IndexBarActivity" />
        <activity android:name=".android.activity.CutomViewActivity" />
        <activity android:name=".android.activity.OkhttpActivity" />
        <activity android:name=".android.activity.SwipeRefreshLayoutActivity" />
        <activity android:name=".android.activity.DesignModeActivity" />
        <activity android:name=".android.activity.RxjavaActivity" />
        <activity android:name=".android.activity.ThreadActivity" />
        <activity android:name=".android.activity.SecondActivity" />
        <activity android:name=".android.activity.JsonXmlActivity" />
        <activity android:name=".android.activity.HomeActivity" />
        <activity android:name=".android.activity.LoaderActivity" />
        <activity android:name=".android.view.RefreshAndMoreActivity1" />
